{
  "presets": ["es2015", "stage-2"],
  "plugins": [
  	"transform-runtime", 
  	["babel-root-import", [{
  		"rootPathPrefix": "~",
  		"rootPathSuffix": "src"
  	}, {
  		"rootPathPrefix": "@",
  		"rootPathSuffix": "src/components"
  	}, {
  		"rootPathPrefix": "$",
  		"rootPathSuffix": "src/plugins"
  	}, {
  		"rootPathPrefix": "#",
  		"rootPathSuffix": "src/store"
  	}]]
  ],
  "comments": false
}
